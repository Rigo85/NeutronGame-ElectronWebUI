{
  "name": "neutrongame-electronwebui",
  "productName": "Neutron Game",
  "version": "1.0.0",
  "description": "Neutron Game",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Neutron Game\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Rigoberto L. Salgado Reyes",
  "license": "AGPL-3.0",
  "dependencies": {
    "electron": "^2.0.6",
    "electron-settings": "^3.2.0",
    "jquery": "^3.3.1",
    "jsonfile": "^4.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "util": "^0.11.0"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}
